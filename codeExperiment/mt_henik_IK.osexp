define inline_script for_if_ypos
	set description "Executes Python code"
	set _run ""
	___prepare__
	var.l1 = list()
	for i in range(1,1000):
		if var.ypos_mousetrap_main[i]>128: 
			var.l1[i] = 1 
	var.sum_l1 =  sum(var.l1)
	for i in range(1, var.sum_l1):
		var.ypos_mousetrap_main[i] = 0
	for i in range(1, var.sum_l1):
		var.xpos_mousetrap_main[i] = 0
	__end__

