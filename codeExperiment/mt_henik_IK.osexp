define sketchpad Intro_main
	set duration keypress
	set description "Intro main study"
	draw textline center=1 color=black font_bold=no font_family=mono font_italic=no font_size=28 html=yes show_if=always text="Now the experiment begins. <br /><br />If you have any questions, please contact the investigator. <br /><br />Press the space bar to start the experiment." x=0 y=0 z_index=0

define sketchpad Intro_start
	set duration keypress
	set description "welcome screen"
	draw textline center=1 color=black font_bold=no font_family=mono font_italic=no font_size=28 html=yes show_if=always text="Welcome! <br /><br />Thank you very much for taking the time for our study.<br /><br />Press the space bar to continue.<br />" x=0 y=0 z_index=0

define sketchpad Intro_start_2
	set duration keypress
	set description "task description"
	draw textline center=1 color=black font_bold=no font_family=mono font_italic=no font_size=28 html=yes show_if=always text="In this study, you are presented with two different numbers in the range 1 to 9. The two numbers differ in height. For example, 6 would be higher than 5. <br /><br />Additionally, the two numbers will also differ in their presented visual size. <br /><br />In the two upper corners of the screen, there are two square fields. <br /><br />Your task is to move the cursor (the mouse) as fast as possible into the square field on the side of the HIGHER number. If the cursor is in the square you do NOT have to press any key. <br /><br />This means that if the numbers 4 and 5 were presented, the cursor should be moved as quickly as possible to the square field on the side of 5. <br /><br />Press the space bar to continue. " x=0 y=0 z_index=0

